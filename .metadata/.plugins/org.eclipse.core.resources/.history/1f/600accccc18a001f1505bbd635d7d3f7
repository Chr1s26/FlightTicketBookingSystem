package view;

import java.awt.BorderLayout;

import javax.swing.JFrame;
import javax.swing.JMenu;
import javax.swing.JMenuBar;

public class HomeView {
	private JFrame homePage;
	private JMenuBar homeMenu;
	private JMenu flightScheduleMenu;
	private JMenu ticketMenu;
	
	public HomeView() {
		this.homePage = new JFrame("Home Page");
		this.homePage.setLayout(new BorderLayout());
		this.homePage.setLocation(200,300);
		this.homePage.setSize(400,400);
		this.homePage.setVisible(true);
	}
}
